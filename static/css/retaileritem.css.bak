.box{
    padding:10px;
    border-top:#0A3151 2px solid;
    border-bottom:#0A3151 2px solid;
}

.img{
    padding-bottom:10%;
}

.items{
    display: inline-block;
    padding:5px;
    padding-top:5%;
    padding-bottom:5%;
    margin:15px;
    width: 25%;
    overflow: hidden;
    text-align: center;
    border-radius:5px;
    border: #0A3151 2px solid;
}

.items:hover{
    border: #3366CC 2px solid;
    box-shadow: 3px 3px 3px 3px #9D9D9D;
}

.anchorc{
    color: white;
}
